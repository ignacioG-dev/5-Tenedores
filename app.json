{
  "expo": {
    "name": "5-tenedores",
    "slug": "5-tenedores",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "cincotenedores",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a680"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIndentifier": "com.ignaciogauto.cincotenedores"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ignaciogauto.cincotenedores"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicacion 5-Tenedores necesita acceder a tus fotos para poder funcionar correctamente."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "32cbdb9c-f703-4baf-8672-20b9ef8438c1"
      }
    }
  }
}
